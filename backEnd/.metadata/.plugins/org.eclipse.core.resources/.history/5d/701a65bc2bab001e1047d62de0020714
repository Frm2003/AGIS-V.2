package com.br.fatec.AGIS.model;

import java.time.LocalDate;

import jakarta.persistence.Column;
import jakarta.persistence.DiscriminatorValue;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import lombok.Getter;
import lombok.Setter;

@Entity
@Getter
@Setter
@DiscriminatorValue(value = "professor")
public class Professor extends Usuario {
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(nullable = false)
	private Long cod;

	@Column(nullable = false, length = 100)
	protected String titulacao;

	public Professor(String cpf, String nome, LocalDate dataNasc, String emailPessoal, String emailCorp, String situacao) {
		super(String cpf, nome, dataNasc, emailPessoal, String emailCorp, String situacao));
	}

}
